# Maintainer: Svintoo <archlinux@svintoo.com>

pkgname=locale-en_se
pkgver=0.0.8
pkgrel=1
pkgdesc="Adds locale en_SE. By using it you get english ISO-8601 locale compatibility in Qt5 applications."
arch=('any')
url=""
license=("Public Domain")
depends=('glibc')
install="$pkgname.install"
source=("$pkgname.hook"
        "$pkgname.sh")
md5sums=('bb992fb87aff6c1f7fa9552509ee7eb9' 
         '09f51917369c602da16df928dac51560')
sha256sums=('333204a47f2b2d70dfab3b087ab6ca271427801a64462a8a7c31f07dd1d0a11e'
            '93ad86296d7d6103c490e870d1c79b8ff4eeacbb70edc44ce08f0665cdf7ef73')

package() {
  mkdir -p "$pkgdir"/etc/pacman.d/hooks/
  install -Dm644 "${srcdir}/${pkgname}.hook" "${pkgdir}/etc/pacman.d/hooks/${pkgname}.hook"
  install -Dm744 "${srcdir}/${pkgname}.sh"   "${pkgdir}/etc/pacman.d/hooks/${pkgname}.sh"
}
